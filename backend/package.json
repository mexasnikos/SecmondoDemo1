{
  "name": "travel-insurance-backend",
  "version": "1.0.0",
  "description": "Backend API for Travel Insurance Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node setup-db.js",
    "test": "node test-db.js",
    "test-db": "node test-db.js",
    "db:setup": "node setup-db.js",
    "db:reset": "psql -U postgres -c \"DROP DATABASE IF EXISTS travel_insurance; CREATE DATABASE travel_insurance;\" && npm run db:setup"
  },
  "keywords": [
    "travel",
    "insurance",
    "api",
    "nodejs",
    "postgresql",
    "express"
  ],
  "author": "Travel Insurance Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^3.0.1",
    "pg": "^8.11.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.1",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
